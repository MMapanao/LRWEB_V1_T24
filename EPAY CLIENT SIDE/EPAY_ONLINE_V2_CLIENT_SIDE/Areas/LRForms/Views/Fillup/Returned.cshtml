    function updateList() {

        var input = document.getElementById('additionalattachment');
        var output = document.getElementById('fileList');
        var children = "";
        output.innerHTML = children;
        if ($("#additionalattachment")[0].files.length > 3) {
            input.val = "";
            alert("You can select only 3 images");

        } else {
            for (var i = 0; i < input.files.length; ++i) {
                children += '<p>' + input.files.item(i).name + '</p>';
            }
            output.innerHTML = children;

        }

    }

    function createCIF() {
        var sample = new jsPDF('l', 'pt', 'a4');
        let halfX = sample.internal.pageSize.width / 2;
        var totalPagesExp = "{total_pages_count_string}";
        sample.autoTable({
            html: '#CIF',
            startY: 20,
            useCss: true,
            styles: { fontSize: 6 },
            columnStyles: {
                //cellWidth: 'wrap'
                0: { cellWidth: 80 },
                1: { cellWidth: 80 },
                2: { cellWidth: 80 },
                3: { cellWidth: 80 },
                4: { cellWidth: 80 },
                5: { cellWidth: 80 },
                6: { cellWidth: 80 },
                7: { cellWidth: 80 }
            },
            didDrawPage: function (data) {
                // Footer
                var str = 'Page ' + sample.internal.getNumberOfPages()

                if (typeof sample.putTotalPages === 'function') {
                    str = str + " of " + totalPagesExp;
                }
                sample.setFontSize(6)

                // jsPDF 1.4+ uses getWidth, <1.4 uses .width
                var pageSize = sample.internal.pageSize
                var pageHeight = pageSize.height ? pageSize.height : pageSize.getHeight()
                sample.text(str, data.settings.margin.left, pageHeight - 10)
            }
        })
        if (typeof sample.putTotalPages === 'function') {
            sample.putTotalPages(totalPagesExp);
        }
        var sampl2 = sample.output('blob');
        //window.open(URL.createObjectURL(sampl2))
        //sample.save("wow.pdf")
        //var file = URL.createObjectURL(sampl2);
        return sampl2;
    }
