<script type="text/javascript">

    function PMT(ir, np, pv, fv, type) {
        /*
        ir - interest rate per month
        np - number of periods (months)
        pv - present value
        fv - future value (residual value)
        */
        //pmt = (ir * (pv * Math.pow((ir + 1), np) + fv)) / ((ir + 1) * (Math.pow((ir + 1), np) - 1));
        var pmt, pvif;

        fv || (fv = 0);
        type || (type = 0);

        if (ir === 0)
            return -(pv + fv) / np;

        pvif = Math.pow(1 + ir, np);
        pmt = - ir * pv * (pvif + fv) / (pvif - 1);

        if (type === 1)
            pmt /= (1 + ir);
        return pmt;
    }
    function NTHP() {
        var netpay = $('#netPay').val().replaceAll(",", "");
        var otherLoans = $('#otherLoans').val().replaceAll(",", "");
        var netIncome = netpay - otherLoans
        $('#netIncome').val(netIncome.toFixed(2)).trigger('change');
    }
    function compute() {
        var interestRate = $('#interestRate').val() / 100;
        var bankCharge = $('#bankCharge').val();
        var netIncome = $('#netIncome').val().replaceAll(",", "");
        var loanAmount = $('#loanAmount').val().replaceAll(",", "");
        var loanTerms = $('#loanTerms').val();
        var monthlyAmortization = -PMT(interestRate / 12, loanTerms, loanAmount, 0, 0).toFixed(2);
        var cr = $("#cr").html() / 100


        //bankinterest/12,term,amount,0,0
        var creditRatio = netIncome * cr
        var BCT = loanAmount * bankCharge / 100;
        var netProceed = loanAmount - BCT
        $("#creditRatio").val(creditRatio.toFixed(2)).trigger('change');
        $("#BCT").val(BCT.toFixed(2)).trigger('change');
        $("#netProceed").val(netProceed.toFixed(2)).trigger('change');
        $("#monthlyAmortization").val(monthlyAmortization.toFixed(2)).trigger('change');


    }

